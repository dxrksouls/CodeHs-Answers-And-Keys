# This has karel build two towers, with three balls in each tower.
# Should be the same program with turn_right and turn_around removed.
def right():
    turn_left()
    turn_left()
    turn_left()
def tower():
    move()
    put_ball()
    turn_left()
    move()
    put_ball()
    move()
    put_ball()
def tower2():
    move()
    right()
    move()
    put_ball()
    move()
    put_ball()
    move()
    put_ball()
    turn_left()
    turn_left()
    move()
    move()
    move()
    right()

tower()
move()
right()
move()
tower2()
